<div class='container'>
  <div class="row" style="margin: 30px 0px ; ">
    <div class="col-md-12">
      <div class="page_title_box d-flex align-items-center">
        <span class="page_title">
          <strong>注文履歴一覧</strong>
        </span>
      </div>
    </div>
  </div>
  
  <div class='col-md-12'>
    <table class='table'>
      <thead>
        <tr>
          <th bgcolor="#EEEEEE">注文日</th>
          <th bgcolor="#EEEEEE">配送先</th>
          <th bgcolor="#EEEEEE">注文商品</th>
          <th bgcolor="#EEEEEE">支払い金額</th>
          <th bgcolor="#EEEEEE">ステータス</th>
          <th bgcolor="#EEEEEE">注文詳細</th>
        </tr>
      </thead>
        <tbody>
          <% @orders.each do |order| %>
            <tr>
              <td><%= order.created_at%></td>
              <td><%= order.postal_code %><br><%= order.address %></td>
              <td><%#=  %></td>
              <td><%#=  %></td>
              <td><%#=  %></td>
              <td><%= link_to '変更', "#", class: "btn btn-sm btn-success" %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
    <div class="col-md-3 text-center ml-auto mr-auto">
      <%= paginate @orders %>
    </div>
 